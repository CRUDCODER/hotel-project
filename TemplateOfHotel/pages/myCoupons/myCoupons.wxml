<view class="page">

        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_example " wx:for="{{coupons}}">
                <view class="weui-cell__hd">
                    <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">{{item.couponsName}}</view>
                <view class="weui-cell__ft" wx:if="{{item.exchangeFlag==1}}">未使用</view>
                <view class="weui-cell__ft" wx:if="{{item.exchangeFlag==0}}">已使用</view>
            </view>

        </view>
</view>